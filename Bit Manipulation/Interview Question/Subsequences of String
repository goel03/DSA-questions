#include <bits/stdc++.h> 
vector<string> subsequences(string str){
	
	 vector<string> ans;

    int n=str.size();

    for(int i=1;i<((1<<n));i++){

        string s="";

        for(int j=0;j<n;j++){

            if((i&(1<<j))!=0){

                s+=str[j];

            }

        }

        ans.push_back(s);

        

    }

    return ans;

	
}
